<%= simple_form_for @medicine, :html =>{ :multipart => true } do |f| %>
	<%= select_tag(:category_id, options_for_select(@categories), :prompt => "Select add category") %>
	<%= f.file_field :medicine_img %>
	<%= f.input :name %>
	<%= f.input :price %>
	<%= f.input :description %> 
	<%= f.button :submit %>
<% end %>
